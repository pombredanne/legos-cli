    "version": "0.2.0",
    "configurations": [
        {
            "name": "track option debug",
            "type": "python",
            "request": "launch",
            "stopOnEntry": true,
            "pythonPath": "${config:python.pythonPath}",
            "program": "${file}",
            "cwd": "",
            "console": "integratedTerminal",
            "env": {},
            "envFile": "${workspaceRoot}/.env",
            "debugOptions": [
                "WaitOnAbnormalExit",
                "WaitOnNormalExit"
            ],
            "args": [
                "track",
                "--repo",
                "prashantvc/legos-cli"
            ]
        },
        {
            "name": "track list",
            "type": "python",
